#include <stdio.h>

int main()
{
    // Write a C program to find factorial of a number taken as an input from user.
    
    int fact=1, input;
    
    printf("Enter a number to calculate factorial: ");
    scanf("%d", &input);
    
    for (int i = 1; i <= input; i++)
    {
        fact *= i;
    }
    
    printf("Factorial of %d is %d", input, fact);

    return 0;
}
