#include<stdio.h>

void main()
{
    // print the first 10 numbers of the fabonacci sequence
    // starting with 0 and 1. third number is the sum of the previous 2 nums 
    int arr[10];
    arr[0] = 0;
    arr[1] = 1;
    
    for (int i = 0; i < 8; i++) // go from 0 to 7 index because last 2 will cause out of bound erro
    {
        arr[i+2] = arr[i]+arr[i+1];
    }
    printf("Fabonacci array: ");
    for (int j = 0; j < 10; j++)
    {
        printf("%d ", arr[j]);
    }
    
    
    
    
}
